{
    "name": "Bridge Extension Example",
    "description": "There's gonna be cats in here",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "cat-512.png",
        "48": "cat-512.png",
        "128": "cat-512.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["index.css"],
          "js": ["script.js"]
        }
      ],
}